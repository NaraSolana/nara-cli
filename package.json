{
  "name": "nara-cli",
  "version": "0.1.0",
  "description": "CLI for the Nara chain (Solana-compatible)",
  "module": "index.ts",
  "main": "index.ts",
  "type": "module",
  "private": false,
  "bin": {
    "nara-cli": "./dist/nara-cli.js"
  },
  "scripts": {
    "cli": "npx tsx bin/nara-cli.ts",
    "build": "npx esbuild bin/nara-cli.ts --bundle --platform=node --format=esm --outfile=dist/nara-cli.js --external:@coral-xyz/anchor --external:@solana/web3.js --external:@solana/spl-token --external:snarkjs --external:@meteora-ag/* --banner:js='#!/usr/bin/env node'",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "src",
    "index.ts",
    "bin"
  ],
  "keywords": [
    "nara",
    "solana",
    "blockchain",
    "cli",
    "sdk"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/bn.js": "^5.2.0",
    "@types/node": "^22.0.0",
    "esbuild": "^0.24.0",
    "tsx": "^4.19.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.32.1",
    "@meteora-ag/cp-amm-sdk": "^1.3.3",
    "@meteora-ag/dynamic-bonding-curve-sdk": "^1.5.2",
    "@solana/spl-token": "^0.4.14",
    "@solana/web3.js": "^1.98.4",
    "bip39": "^3.1.0",
    "bn.js": "^5.2.2",
    "bs58": "^6.0.0",
    "commander": "^12.1.0",
    "ed25519-hd-key": "^1.3.0",
    "snarkjs": "^0.7.6"
  }
}
